{
    "name": "Dev Container Compose",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/workspaces/Dev-Container-Compose",
    "postStartCommand": "pip install -r requirements.txt",
    "waitFor": "onCreateCommand",
    "updateContentCommand": "python3 -m pip install -r requirements.txt && alias py='python'",
    "postCreateCommand": "",
    "customizations": {
        "codespaces": {
        "openFiles": ["main.ipynb"]
        },
        "vscode": {
        "extensions": [
            "ms-toolsai.jupyter",
            "ms-python.python"
        ]
        }
    }
}